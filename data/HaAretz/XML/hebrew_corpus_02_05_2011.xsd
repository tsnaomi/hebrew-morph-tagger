		</xsd:annotation>
		<xsd:attribute name="register" type="RegisterType" default="formal"/>
		<xsd:attribute name="spelling" type="SpellingType" default="standard"/>
	</xsd:complexType>
	<xsd:complexType name="WprefixType">
