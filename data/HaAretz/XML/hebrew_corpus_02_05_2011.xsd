		<!-- מין  -->
		<!-- מספר -->
		<!-- גוף  -->
		<!-- שורש                 -->
		<!-- בניין                -->
		<!-- ז'אנר                -->
	</xsd:complexType>
	<xsd:simpleType name="BinyanType">
